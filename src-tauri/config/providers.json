{
  "version": "1.0.0",
  "providers": {
    "ChatGPT": {
      "provider_id": "ChatGPT",
      "config_version": "1.0.0",
      "input_selectors": [
        "textarea[data-id='root']",
        "textarea[placeholder*='Message']",
        "#prompt-textarea"
      ],
      "submit_selectors": [
        "button[data-testid='send-button']",
        "button[aria-label='Send message']",
        "button[type='submit']"
      ],
      "auth_check_selectors": [
        ".login-button",
        "button[data-testid='login-button']",
        "#__next [href='/auth/login']"
      ],
      "last_updated": "2025-11-13T00:00:00Z",
      "notes": "Selectors tested against ChatGPT as of November 2025. Primary selector uses data-id attribute."
    },
    "Gemini": {
      "provider_id": "Gemini",
      "config_version": "1.0.0",
      "input_selectors": [
        ".ql-editor[contenteditable='true']",
        "div[role='textbox'][aria-label*='Enter a prompt']",
        ".input-area textarea"
      ],
      "submit_selectors": [
        "button[aria-label='Send']",
        "button[mattooltip='Send']",
        ".send-button"
      ],
      "auth_check_selectors": [
        "a[href*='accounts.google.com/signin']",
        ".sign-in-button",
        "button[aria-label='Sign in']"
      ],
      "last_updated": "2025-11-13T00:00:00Z",
      "notes": "Gemini uses contenteditable div instead of textarea. May use Google Material Design components."
    },
    "Claude": {
      "provider_id": "Claude",
      "config_version": "1.0.0",
      "input_selectors": [
        "div[contenteditable='true'][role='textbox']",
        ".ProseMirror[contenteditable='true']",
        "fieldset textarea"
      ],
      "submit_selectors": [
        "button[aria-label='Send message']",
        "button[type='button'][aria-label*='Send']",
        "fieldset button[type='button']",
        "button[type='submit']",
        ".send-button"
      ],
      "auth_check_selectors": [
        "a[href*='/login']",
        ".login-prompt",
        "button:has-text('Log in')"
      ],
      "last_updated": "2025-11-13T00:00:00Z",
      "notes": "Claude uses ProseMirror editor (contenteditable div). Selectors may need updates as UI evolves."
    }
  }
}
