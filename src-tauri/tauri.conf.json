{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ChenChen",
  "version": "0.1.3",
  "identifier": "com.chenchen.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "ChenChen - Multi-LLM Prompt",
        "width": 1200,
        "height": 900,
        "minWidth": 800,
        "minHeight": 700,
        "transparent": false,
        "theme": "Dark"
      }
    ],
    "withGlobalTauri": true,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "config/providers.json"
    ]
  }
}
